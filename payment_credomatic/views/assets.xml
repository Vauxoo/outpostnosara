<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_frontend" inherit_id="website.assets_frontend">
        <xpath expr="//link[last()]" position="before">
            <link rel="stylesheet" type="text/scss" href="/payment_credomatic/static/src/scss/main.scss" />
        </xpath>
        <xpath expr="." position="inside">
            <script src="/payment_credomatic/static/src/js/credomatic_s2s_form.js" type="text/javascript"/>
        </xpath>
    </template>
    <template id="assets_backend" inherit_id="web.assets_backend">
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/payment_credomatic/static/src/js/actions.js" />
        </xpath>
    </template>
</odoo>
